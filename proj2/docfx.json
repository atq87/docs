{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/bin/Debug/**.dll"
          ],
          "exclude": [
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api"
    },
    {
      "src": "src2/**.cs",
      "dest": "api-vb"
    } 	
  ],
  "build": {
    "markdownEngineName": "markdig",
    "content": [

      {
        "files": [
          "**index.md",
          "**toc.yml",
          "**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "**_site/**",
          "**proj2/**",
          "**posts/**",
          "readme.md"
        ],
      "cwd": "../"
    }
    ],

    "resource": [
      {
        "files": [
          "bimlflex/user-guide/images/**",
          "bimlexpress/images/**",
          "content/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "**_site/**",
          "**proj2/**"
        ],
        "cwd": "../"
      }
    ],

    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],

    "dest": "_site",
	"template": [
		"default"
    ]
  }
}